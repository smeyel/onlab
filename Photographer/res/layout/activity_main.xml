         <TextView
             android:id="@+id/TextView01"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentLeft="true"
             android:layout_below="@+id/btnHttpBasicGet"
             android:layout_toLeftOf="@+id/btnHttpBasicGet"
             android:text="@string/Text" />
    
    <ImageView
        android:id="@+id/ivPhoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone"
        android:contentDescription="@string/ivPhoto"
        />
    <Button
        android:id="@+id/buttonPhoto"
        android:text="@string/btnCapture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <Button
        android:id="@+id/btnHttpBasicGet"
        android:layout_width="171dp"
        android:layout_height="132dp"
        android:layout_alignBottom="@+id/buttonPhoto"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="@string/btnReg" />

